is_leap_year = False

print("What year to check? ")
year = int(input())

if year % 4 == 0:
  is_leap_year = True


if year % 100 == 0:

  is_leap_year = False

  if year % 400 == 0:

    is_leap_year = True


if is_leap_year:
  print(year,"is a leap year.")
else:
  print(year,"is not a leap year.")
